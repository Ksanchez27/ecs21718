        <script>
            google.load("visualization", "1", {packages:["corechart"]});
            var poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_voted = sessionStorage.getItem("poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195");
            var poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_options = {
                legend: {position:'right'},
                width: '100%',
                height: '100%',
                title: 'Should DACA still be in place?',
                hAxis: {minValue:0},
                colors: ['#9CCF9C', '#9C9ACE', '#9C6563']
            };

            function poll_results_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195(bBackBtn){
                $.ajax({
                    type: 'GET',
                    url: '/should-daca-still-be-in-place/poll_ce8a003e-fa52-11e7-bac2-0b83d31a60ec.html?action=poll:results',
                    dataType: 'json',
                    success: function(result){
                        var data = [];
                        data.push(['Answer', 'Votes', {role:'style'}]);
                        $.each(result.results,function(i,item){
                            var iNumVotes = item.votes;
                            if( iNumVotes === 0 && item.text === poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_voted ){
                                iNumVotes = 1;
                            }
                            data.push([item.text, iNumVotes, item.color]);
                        });
                        $poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_data = google.visualization.arrayToDataTable(data);

                        
                            $poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_chart = new google.visualization.PieChart(document.getElementById('poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-results'));
                        

                        $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-results').show();

                        $poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_chart.draw($poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_data, poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_options);

                        $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-your-vote').html( poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_voted );
                        $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-container').hide();

                        if(bBackBtn){
                            $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-back-vote').removeClass('hide');
                        } else {
                            $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-your-vote-container').show();
                        }
                    }
                });
            }

            var poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_resized;
            jQuery(window).on('resize orientationChanged', function(){
                if( $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-results').is(':visible') ){
                    clearTimeout(poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_resized);
                    var poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_resized = setTimeout(function(){
                        $poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_chart.draw($poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_data, poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_options);
                    }, 400);
                }
            });

            jQuery(document).ready(function($){
                if( poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_voted ) {
                    poll_results_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195();
                }

                $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-get-results').click(function(){
                    poll_results_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195(true);
                    return false;
                });

                $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-back-btn').click(function(){
                    $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-back-vote').addClass('hide');
                    $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-results').hide();
                    $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-container').show();
                });

                $('#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-form').submit(function(){
                    var pollAnswer = $("#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-form input[name=answer]:checked").val();
                    var pollAnswerNice = $("#poll-ce8a003e-fa52-11e7-bac2-0b83d31a60ec-489195-form input[name=answer]:checked").attr('title');
                    var pollDmpData = 'Poll : Should DACA still be in place? : ' + pollAnswerNice;
                    
                    if(!pollAnswer){
                        return false;
                    }
                    $.ajax({
                        type: 'POST',
                        url: '/should-daca-still-be-in-place/poll_ce8a003e-fa52-11e7-bac2-0b83d31a60ec.html',
                        data: 'format=json&action=poll:vote&answer='+pollAnswer,
                        success: function(result){
                            try{
                                sessionStorage.setItem("poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195", pollAnswerNice);
                            }catch(e){};
                            poll_ce8a003e_fa52_11e7_bac2_0b83d31a60ec_489195_voted = pollAnswerNice;

<script>
    $('#site-search-489211-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-489211').submit();
        return false;
    });

    $('#site-search-489211').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-489211 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-489211 input[name=q]").val( sQueryFiltered );
                
        if( $('#site-search-489211-term').val() ){
            $('#site-search-489211-btn .fa').removeClass('fa-search').addClass('fa-spinner fa-spin');
            return true;
        }else{
            return false;
        }
    });</script></div></div>
                                

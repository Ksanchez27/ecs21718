<script>
    $('#site-search-489212-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-489212').submit();
        return false;
    });

    $('#site-search-489212').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-489212 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-489212 input[name=q]").val( sQueryFiltered );
                
        if( $('#site-search-489212-term').val() ){
            $('#site-search-489212-btn .fa').removeClass('fa-search').addClass('fa-spinner fa-spin');
            return true;
        }else{
            return false;
        }
    });</script></div></div>
